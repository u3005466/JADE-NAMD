#! /usr/bin/env bash
# suppose the JADE_HOME env. is exported
#
# i.e. 
export JADE_HOME=/data/home/hudp/program/jade_mapping

# binary
export PATH=$JADE_HOME/bin:$PATH

# # interface
export JADE_HOME_INTERFACE=$JADE_HOME/src/electronic
export PATH=$JADE_HOME_INTERFACE:$PATH
## kkr
export JADE_HOME_INTERFACE=$JADE_HOME/src/krr
export PATH=$JADE_HOME_INTERFACE:$PATH
# # sampling
export JADE_HOME_SAMPLE=$JADE_HOME/src/sampling2
export PATH=$JADE_HOME_SAMPLE:$PATH
# # pop
export JADE_POPULATION=$JADE_HOME/contrib/population_ana
export PATH=$JADE_POPULATION:$PATH
# # contrib : analysis tools
export JADE_CONTRIB=$JADE_HOME/contrib
export PATH=$JADE_CONTRIB:$PATH
#
